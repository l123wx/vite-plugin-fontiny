{
  "name": "vite-plugin-fontiny",
  "version": "0.1.1",
  "packageManager": "pnpm@9.0.5",
  "description": "A font file compression tool for Vite.",
  "author": "elvis <https://github.com/l123wx>",
  "license": "MIT",
  "homepage": "https://github.com/l123wx/vite-plugin-fontiny",
  "repository": {
    "type": "git",
    "url": "https://github.com/l123wx/vite-plugin-fontiny"
  },
  "bugs": "https://github.com/l123wx/vite-plugin-fontiny/issues",
  "keywords": [
    "fontiny",
    "fontmin",
    "vite",
    "plugin"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "npm run build -- --watch",
    "example:start": "npm -C example run start",
    "build": "tsup src/index.ts --dts --format cjs,esm",
    "prepublishOnly": "npm run build",
    "release": "npx bumpp && npm publish",
    "lint": "eslint ."
  },
  "dependencies": {
    "fontmin": "^1.1.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.21.1",
    "@rollup/pluginutils": "^5.1.0",
    "@types/node": "^20.14.9",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "eslint": "^8.56.0",
    "rollup": "^4.18.0",
    "tsup": "^8.1.0",
    "typescript": "^5.5.2",
    "vite": "^5.3.1"
  }
}
